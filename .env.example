# Przykładowa konfiguracja środowiska dla Greenbone
# Skopiuj ten plik jako .env i uzupełnij swoimi danymi

# SMTP (do wysyłki raportów)
MTA_HOST=smtp.example.com
MTA_PORT=587
MTA_TLS=on
MTA_STARTTLS=on
MTA_AUTH=on
MTA_USER=twoj_email@example.com
MTA_FROM=twoj_email@example.com
MTA_PASSWORD=haslo_lub_token

# Dane Greenbone
GVM_USER=admin
GVM_PASS=haslo
RECIPIENT=odbiorca@example.com
SENDER=nadawca@example.com
SCAN_TARGETS=192.168.1.0/24
PORTS=T:1-65535
